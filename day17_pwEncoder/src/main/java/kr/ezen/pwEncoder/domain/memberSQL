-- 유저 테이블
drop table member;
create table member(
    no int auto_increment primary key,
    id varchar(20) not null,
    pw varchar(20) not null,
    name varchar(20) not null,
    age int not null,
    email varchar(30) not null,
    tel varchar(20) not null
);
commit;
select * from member;
desc member;